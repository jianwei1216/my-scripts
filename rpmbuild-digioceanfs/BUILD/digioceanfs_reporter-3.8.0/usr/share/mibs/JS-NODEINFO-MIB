NODEINFO-MIB DEFINITIONS ::= BEGIN

-- imports and exports

IMPORTS
    MODULE-IDENTITY, OBJECT-TYPE, Integer32, Gauge32
        FROM SNMPv2-SMI
    DisplayString, TruthValue
        FROM SNMPv2-TC
    hackingExperimental
        FROM HACKING-SNMP-MIB;

NodeInfoMIB MODULE-IDENTITY
    LAST-UPDATED    "201508140000Z" 
    ORGANIZATION    "Hacking Node Solutions Ltd"
    CONTACT-INFO    
        "Max Hacking
         DOFS Storage Management System
         Perabytes Technology Co., Ltd. Copy Right
         Website: http://www.perabytes.com
         Hot Line: 400-656-1980
        "
    DESCRIPTION     
        "This MIB module defines objects for node info."
    REVISION        "201603030000Z"
    DESCRIPTION     
        "Modify field type."
    ::= { NodeInfo 1 }

NodeInfo      OBJECT IDENTIFIER ::= { hackingExperimental 20 }

-- NodeStateTable

NodeStateTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF SNodeStateEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "Table of node information."
    ::= { NodeInfo 2 }

NodeStateEntry OBJECT-TYPE
    SYNTAX      SNodeStateEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "An entry containing a node and its info."
    INDEX       { NodeStateIndex }
    ::= { NodeStateTable 1 }

    SNodeStateEntry ::= SEQUENCE {
        NodeStateIndex        Integer32,
        NodeStateName         DisplayString,
        NodeStateIP           DisplayString,
        NodeStateUp           INTEGER,
}

NodeStateIndex OBJECT-TYPE
    SYNTAX      Integer32 (0..65535)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Reference index for each observed node."
    ::= { NodeStateEntry 1 }

NodeStateName OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Node name"
    ::= { NodeStateEntry 2 }

NodeStateIP OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "IP Address of node"
    ::= { NodeStateEntry 3 }

NodeStateUp OBJECT-TYPE
    SYNTAX      INTEGER{start(1),stop(0)}
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Status of node"
    ::= { NodeStateEntry 4 }

