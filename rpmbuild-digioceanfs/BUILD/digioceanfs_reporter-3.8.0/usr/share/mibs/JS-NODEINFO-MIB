    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "An entry containing a device and its statistics."
    INDEX       { NodeNFSStatusIndex }
    ::= { NodeNFSStatusTable 1 }

    SNodeNFSStatusEntry ::= SEQUENCE {
        NodeNFSStatusIndex    Integer32,
        NodeNFSStatusName     DisplayString,
        NodeNFSStatusUp       INTEGER,
}

NodeNFSStatusIndex OBJECT-TYPE
    SYNTAX      Integer32 (0..65535)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Reference index for each observed device."
    ::= { NodeNFSStatusEntry 1 }

NodeNFSStatusName OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Node Name"
    ::= { NodeNFSStatusEntry 2 }

NodeNFSStatusUp OBJECT-TYPE
    SYNTAX      INTEGER{start(1),stop(0)}
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "NFS Status"
    ::= { NodeNFSStatusEntry 3 }

