SERVICEINFO-MIB DEFINITIONS ::= BEGIN

-- imports and exports

IMPORTS
    MODULE-IDENTITY, OBJECT-TYPE, Integer32, Gauge32
        FROM SNMPv2-SMI
    DisplayString, TruthValue
        FROM SNMPv2-TC
    hackingExperimental
        FROM HACKING-SNMP-MIB;

ServiceInfoMIB MODULE-IDENTITY
        LAST-UPDATED    "201508140000Z"
        ORGANIZATION    "Hacking Service Solutions Ltd"
        CONTACT-INFO    
            "Max Hacking
             DOFS Storage Management System
             Perabytes Technology Co., Ltd. Copy Right
             Website: http://www.perabytes.com
             Hot Line: 400-656-1980
            "
        DESCRIPTION     
            "This MIB module defines objects for service info."
        REVISION        "201603030000Z"
        DESCRIPTION     
            "Modify field type."
    ::= { ServiceInfo 1 }

ServiceInfo      OBJECT IDENTIFIER ::= { hackingExperimental 21 }

-- ServiceStateTable

ServiceStateTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF SServiceStateEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "Table of service information."
    ::= { ServiceInfo 2 }

ServiceStateEntry OBJECT-TYPE
    SYNTAX      SServiceStateEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "An entry containing a service and its statistics."
    INDEX       { ServiceStateIndex }
    ::= { ServiceStateTable 1 }

    SServiceStateEntry ::= SEQUENCE {
        ServiceStateIndex          Integer32,
        ServiceStateName           DisplayString,
        ServiceStateRaidlv         DisplayString,
        ServiceStateVid            DisplayString,
        ServiceStateUp             INTEGER,
        ServiceStateDiskNumber     Gauge32,
        ServiceStateTransportType  DisplayString,
        ServiceStateFreeSize       Gauge32,
        ServiceStateTotalSize      Gauge32,
        ServiceStateUsedSize       Gauge32,
        ServiceStateUsage          Integer32,
        ServiceStateBrickStatus    DisplayString,
        ServiceStateClientNumber   Gauge32,
}

ServiceStateIndex OBJECT-TYPE
    SYNTAX      Integer32 (0..65535)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Reference index for each observed device."
    ::= { ServiceStateEntry 1 }

ServiceStateName OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Service name"
    ::= { ServiceStateEntry 2 }

ServiceStateRaidlv OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Service Level"
    ::= { ServiceStateEntry 3 }

ServiceStateVid OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Volume ID"
    ::= { ServiceStateEntry 4 }

ServiceStateUp OBJECT-TYPE
    SYNTAX      INTEGER{create(0),start(1),stop(-1)}
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Service Status"
    ::= { ServiceStateEntry 5 }

ServiceStateDiskNumber OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The Number of Service Disk"
    ::= { ServiceStateEntry 6 }

ServiceStateTransportType OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Service Transport Type"
    ::= { ServiceStateEntry 7 }

ServiceStateFreeSize OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Service Free Size"
    ::= { ServiceStateEntry 8 }

ServiceStateTotalSize OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Service Total Size"
    ::= { ServiceStateEntry 9 }

ServiceStateUsedSize OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Service Used Size"
    ::= { ServiceStateEntry 10 }

ServiceStateUsage OBJECT-TYPE
    SYNTAX      Integer32(0..100)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Service Size Usage"
    ::= { ServiceStateEntry 11 }

ServiceStateBrickStatus OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Service Brick Status"
    ::= { ServiceStateEntry 12 }

ServiceStateClientNumber OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Service Client Number"
    ::= { ServiceStateEntry 13 }

-- ServiceDiskTable

ServiceDiskTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF SServiceDiskEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "Table of service disk information."
    ::= { ServiceInfo 3 }

ServiceDiskEntry OBJECT-TYPE
    SYNTAX      SServiceDiskEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "An entry containing a service and its statistics."
    INDEX       { ServiceDiskIndex }
    ::= { ServiceDiskTable 1 }

    SServiceDiskEntry ::= SEQUENCE {
        ServiceDiskIndex  Integer32,
        ServiceDiskServiceName   DisplayString,
        ServiceDiskInterface     DisplayString,
        ServiceDiskMark          DisplayString,
        ServiceDiskMountPoint    DisplayString,
        ServiceDiskFS            DisplayString,
        ServiceDiskStatus        INTEGER,
        ServiceDiskTotal         DisplayString,
        ServiceDiskFree          DisplayString,
        ServiceDiskUsage         Integer32,
}

ServiceDiskIndex OBJECT-TYPE
    SYNTAX      Integer32 (0..65535)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Reference index for each observed device."
    ::= { ServiceDiskEntry 1 }

ServiceDiskServiceName OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Service name"
    ::= { ServiceDiskEntry 2 }

ServiceDiskInterface OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Disk Name"
    ::= { ServiceDiskEntry 3 }

ServiceDiskMark OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Disk Mark"
    ::= { ServiceDiskEntry 4 }

ServiceDiskMountPoint OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The Mount Point of Disk"
    ::= { ServiceDiskEntry 5 }

ServiceDiskFS OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The File System of Disk"
    ::= { ServiceDiskEntry 6 }

ServiceDiskStatus OBJECT-TYPE
    SYNTAX      INTEGER{online(1),offline(0)}
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Disk Status"
    ::= { ServiceDiskEntry 7 }

ServiceDiskTotal OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Total Size of Disk"
    ::= { ServiceDiskEntry 8 }

ServiceDiskFree OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Free Size of Disk"
    ::= { ServiceDiskEntry 9 }

ServiceDiskUsage OBJECT-TYPE
    SYNTAX      Integer32(0..100)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Usage of Disk"
    ::= { ServiceDiskEntry 10 }

