    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Quota Directory Name"
    ::= { ServiceQuotaEntry 3 }

ServiceQuotaPath OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Quota Directory Path"
    ::= { ServiceQuotaEntry 4 }

ServiceQuotaTotalSize OBJECT-TYPE
